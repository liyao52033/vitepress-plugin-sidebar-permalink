{
    "name": "vitepress-plugin-sidebar-permalink",
    "version": "1.0.0",
    "private": false,
    "description": "VitePress 自动生成 sidebar 和 permalink rewrites 的插件",
    "type": "module",
    "keywords": [
        "vitepress",
        "sidebar",
        "permalink",
        "plugin"
    ],
    "license": "MIT",
    "files": [
        "dist"
    ],
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.mjs",
            "require": "./dist/*.cjs"
        }
    },
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index.d.ts"
            ]
        }
    },
    "scripts": {
        "build": "unbuild"
    },
    "dependencies": {
        "gray-matter": "^4.0.3"
    },
    "devDependencies": {
        "@types/node": "^22.15.12",
        "typescript": "^5.8.3",
        "unbuild": "^3.2.0"
    },
    "peerDependencies": {
        "vitepress": ">=1.0.0"
    }
}