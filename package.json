{
    "name": "vitepress-plugin-sidebar-permalink",
    "version": "1.3.3",
    "private": false,
    "description": "VitePress 自动生成 sidebar 和 permalink rewrites 的插件",
    "type": "module",
    "keywords": [
        "vitepress",
        "sidebar",
        "permalink",
        "plugin"
    ],
    "license": "MIT",
    "homepage": "https://github.com/liyao52033/vitepress-plugin-sidebar-permalink.git",
    "repository": {
        "type": "git",
        "url": "git@github.com:liyao52033/vitepress-plugin-sidebar-permalink.git"
    },
    "bugs": {
        "url": "https://github.com/liyao52033/vitepress-plugin-sidebar-permalink/issues"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./sidebar": {
            "types": "./dist/sidebar.d.ts",
            "import": "./dist/sidebar.mjs",
            "require": "./dist/sidebar.cjs"
        },
        "./index.css": "./dist/index.css",
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.mjs",
            "require": "./dist/*.cjs"
        }
    },
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index.d.ts"
            ],
            "sidebar": [
                "./dist/sidebar.d.ts"
            ]
        }
    },
    "scripts": {
        "build": "unbuild && copy src\\index.css dist\\index.css",
        "publish": "npm run build && npm publish"
    },
    "dependencies": {
        "gray-matter": "^4.0.3"
    },
    "devDependencies": {
        "@types/node": "^22.15.12",
        "typescript": "^5.8.3",
        "unbuild": "^3.2.0"
    },
    "peerDependencies": {
        "vitepress": ">=1.0.0"
    }
}